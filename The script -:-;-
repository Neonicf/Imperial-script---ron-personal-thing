        local spendingGovernment = getValue(spendingFolder, "Government", true)
        SpendingGovernmentLabel:SetText("Spending (Laws): Government = " .. (spendingGovernment ~= nil and formatNumberWithCommas(spendingGovernment) or "N/A"))

        local spendingHealthcare = getValue(spendingFolder, "Healthcare", true)
        SpendingHealthcareLabel:SetText("Spending (Laws): Healthcare = " .. (spendingHealthcare ~= nil and formatNumberWithCommas(spendingHealthcare) or "N/A"))

        local spendingMilitary = getValue(spendingFolder, "Military", true)
        SpendingMilitaryLabel:SetText("Spending (Laws): Military = " .. (spendingMilitary ~= nil and formatNumberWithCommas(spendingMilitary) or "N/A"))

        local spendingReconstruction = getValue(spendingFolder, "Reconstruction", true)
        SpendingReconstructionLabel:SetText("Spending (Laws): Reconstruction = " .. (spendingReconstruction ~= nil and formatNumberWithCommas(spendingReconstruction) or "N/A"))

        local spendingResearch = getValue(spendingFolder, "Research", true)
