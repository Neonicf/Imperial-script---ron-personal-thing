-- Helper function to format numbers with commas
local function formatNumberWithCommas(number)
    if type(number) ~= "number" then
        return tostring(number)
    end
    local s = tostring(math.floor(number))
    local formatted = ""
    local len = #s
    local count = 0
    for i = len, 1, -1 do
        formatted = s:sub(i, i) .. formatted
        count = count + 1
        if count % 3 == 0 and i ~= 1 then
            formatted = "," .. formatted
        end
    end
    return formatted
end

-- Helper function to get value from instance or attribute
local function getValue(instance, name, isAttribute)
    if not instance then return nil end
    local val = isAttribute and instance:GetAttribute(name) or instance:FindFirstChild(name)
    if val then
        if isAttribute then
            return val
        elseif val:IsA("IntValue") or val:IsA("NumberValue") then
            return val.Value
        elseif val:IsA("StringValue") then
            return val.Value
        elseif val:IsA("Vector3Value") then
            return val.Value.X -- Displaying only X for Vector3Value
        elseif val:IsA("Color3Value") then
            return tostring(val.Value) -- Convert Color3 to string
        elseif val:IsA("ObjectValue") then
            return tostring(val.Value) -- Convert ObjectValue to string (will show instance path)
        end
    end
    return nil
end

-- Variables to manage the refresh task and toggle state
local currentRefreshTask = nil
local currentCountryNameBeingRefreshed = nil
local autoRefreshEnabled = true -- Default state for the toggle

-- Function to refresh all country stats in the UI
local function refreshCountryStats(countryName)
    local countryDataFolder = game.Workspace:FindFirstChild("CountryData") and game.Workspace.CountryData:FindFirstChild(countryName)
    local notFoundText = "N/A (Country not found)"

    if countryDataFolder then
        local economyFolder = countryDataFolder:FindFirstChild("Economy")
        local expensesFolder = economyFolder and economyFolder:FindFirstChild("Expenses")
        local revenueFolder = economyFolder and economyFolder:FindFirstChild("Revenue")
        local powerFolder = countryDataFolder:FindFirstChild("Power")
        local dataFolder = countryDataFolder:FindFirstChild("Data")
        local lawsFolder = countryDataFolder:FindFirstChild("Laws")
        local politicalFolder = lawsFolder and lawsFolder:FindFirstChild("Political")
        local spendingFolder = lawsFolder and lawsFolder:FindFirstChild("Spending")
        local populationFolder = countryDataFolder:FindFirstChild("Population")
        local growthRateInstance = populationFolder and populationFolder:FindFirstChild("GrowthRate")


        -- Economy/Revenue
        local treasury = getValue(economyFolder, "Balance")
        TreasuryLabel:SetText("Treasury/Balance = " .. (treasury ~= nil and formatNumberWithCommas(treasury) or "N/A"))

        local incomeTotal = getValue(revenueFolder, "Total", true)
        IncomeRevenueLabel:SetText("Income/Revenue = " .. (incomeTotal ~= nil and formatNumberWithCommas(incomeTotal) or "N/A"))

        local tax = getValue(revenueFolder, "Tax")
        TaxLabel:SetText("Tax = " .. (tax ~= nil and formatNumberWithCommas(tax) or "N/A"))

        local taxResource = getValue(revenueFolder, "TaxResource")
        TaxResourceLabel:SetText("TaxResource (resources) = " .. (taxResource ~= nil and formatNumberWithCommas(taxResource) or "N/A"))

        local tradeExport = getValue(revenueFolder, "TradeExport")
        TradeExportLabel:SetText("TradeExport = " .. (tradeExport ~= nil and formatNumberWithCommas(tradeExport) or "N/A"))

        local reparationsRevenue = getValue(revenueFolder, "Reparations")
        ReparationsRevenueLabel:SetText("Reparations = " .. (reparationsRevenue ~= nil and formatNumberWithCommas(reparationsRevenue) or "N/A"))

        local puppetsRevenue = getValue(revenueFolder, "Puppets")
        PuppetsRevenueLabel:SetText("Puppets = " .. (puppetsRevenue ~= nil and formatNumberWithCommas(puppetsRevenue) or "N/A"))

        -- Economy/Expenses
        local upkeep = getValue(expensesFolder, "Upkeep")
        UpkeepLabel:SetText("Upkeep = " .. (upkeep ~= nil and formatNumberWithCommas(upkeep) or "N/A"))

        local reinforcements = getValue(expensesFolder, "Reinforcements")
        ReinforcementsLabel:SetText("Reinforcements = " .. (reinforcements ~= nil and formatNumberWithCommas(reinforcements) or "N/A"))

        local tradeImport = getValue(expensesFolder, "TradeImport")
        TradeImportLabel:SetText("TradeImport = " .. (tradeImport ~= nil and formatNumberWithCommas(tradeImport) or "N/A"))

        local reparationsExpenses = getValue(expensesFolder, "Reparations")
        ReparationsExpensesLabel:SetText("Reparations = " .. (reparationsExpenses ~= nil and formatNumberWithCommas(reparationsExpenses) or "N/A"))

        local puppetsExpenses = getValue(expensesFolder, "Puppets")
        PuppetsExpensesLabel:SetText("Puppets = " .. (puppetsExpenses ~= nil and formatNumberWithCommas(puppetsExpenses) or "N/A"))

        local buildingExpenses = getValue(expensesFolder, "BuildingExpenses")
        BuildingExpensesLabel:SetText("Building Expenses = " .. (buildingExpenses ~= nil and formatNumberWithCommas(buildingExpenses) or "N/A"))

        -- Economy-related Spending (from Expenses folder)
        local economySpendingGovernment = getValue(expensesFolder, "Spending_Government")
        GovernmentSpendingLabel:SetText("Government Spending (Economy) = " .. (economySpendingGovernment ~= nil and formatNumberWithCommas(economySpendingGovernment) or "N/A"))

        local economySpendingSecurity = getValue(expensesFolder, "Spending_Security")
        SecuritySpendingLabel:SetText("Security Spending (Economy) = " .. (economySpendingSecurity ~= nil and formatNumberWithCommas(economySpendingSecurity) or "N/A"))

        local economySpendingEducation = getValue(expensesFolder, "Spending_Education")
        EducationSpendingLabel:SetText("Education Spending (Economy) = " .. (economySpendingEducation ~= nil and formatNumberWithCommas(economySpendingEducation) or "N/A"))

        local economySpendingCorruption = getValue(expensesFolder, "Spending_Corruption")
        AntiCorruptionSpendingLabel:SetText("Anti-Corruption Spending (Economy) = " .. (economySpendingCorruption ~= nil and formatNumberWithCommas(economySpendingCorruption) or "N/A"))

        local economySpendingHealthcare = getValue(expensesFolder, "Spending_Healthcare")
        HealthcareSpendingLabel:SetText("Healthcare Spending (Economy) = " .. (economySpendingHealthcare ~= nil and formatNumberWithCommas(economySpendingHealthcare) or "N/A"))

        local economySpendingResearch = getValue(expensesFolder, "Spending_Research")
        ResearchSpendingLabel:SetText("Research Spending (Economy) = " .. (economySpendingResearch ~= nil and formatNumberWithCommas(economySpendingResearch) or "N/A"))

        local economySpendingReconstruction = getValue(expensesFolder, "Spending_Reconstruction")
        ReconstructionSpendingLabel:SetText("Reconstruction Spending (Economy) = " .. (economySpendingReconstruction ~= nil and formatNumberWithCommas(economySpendingReconstruction) or "N/A"))


        -- Power Stats
        local corruptionInstance = powerFolder and powerFolder:FindFirstChild("Corruption")
        local corruptionPower = getValue(powerFolder, "Corruption")
        CorruptionLabel:SetText("Corruption = " .. (corruptionPower ~= nil and formatNumberWithCommas(corruptionPower) or "N/A"))
        local corruptionIncrease = getValue(corruptionInstance, "Increase")
        CorruptionIncreaseLabel:SetText("Corruption (Increase) = " .. (corruptionIncrease ~= nil and formatNumberWithCommas(corruptionIncrease) or "N/A"))

        local militaryInstance = powerFolder and powerFolder:FindFirstChild("Military")
        local militaryPower = getValue(powerFolder, "Military")
        MilitaryLabel:SetText("Military = " .. (militaryPower ~= nil and formatNumberWithCommas(militaryPower) or "N/A"))
        local militaryIncrease = getValue(militaryInstance, "Increase")
        MilitaryIncreaseLabel:SetText("Military (Increase) = " .. (militaryIncrease ~= nil and formatNumberWithCommas(militaryIncrease) or "N/A"))

        local politicalPowerInstance = powerFolder and powerFolder:FindFirstChild("Political")
        local politicalPower = getValue(powerFolder, "Political")
        PoliticalPowerLabel:SetText("Political Power = " .. (politicalPower ~= nil and formatNumberWithCommas(politicalPower) or "N/A"))
        local politicalPowerIncrease = getValue(politicalPowerInstance, "Increase")
        PoliticalPowerIncreaseLabel:SetText("Political Power (Increase) = " .. (politicalPowerIncrease ~= nil and formatNumberWithCommas(politicalPowerIncrease) or "N/A"))

        local readinessInstance = powerFolder and powerFolder:FindFirstChild("Readiness")
        local readinessPower = getValue(powerFolder, "Readiness")
        ReadinessLabel:SetText("Readiness = " .. (readinessPower ~= nil and formatNumberWithCommas(readinessPower) or "N/A"))
        local readinessIncrease = getValue(readinessInstance, "Increase")
        ReadinessIncreaseLabel:SetText("Readiness (Increase) = " .. (readinessIncrease ~= nil and formatNumberWithCommas(readinessIncrease) or "N/A"))

        local researchPowerInstance = powerFolder and powerFolder:FindFirstChild("Research")
        local researchPower = getValue(powerFolder, "Research")
        ResearchPowerLabel:SetText("Research (Power) = " .. (researchPower ~= nil and formatNumberWithCommas(researchPower) or "N/A"))
        local researchIncrease = getValue(researchPowerInstance, "Increase")
        ResearchPowerIncreaseLabel:SetText("Research (Power Increase) = " .. (researchIncrease ~= nil and formatNumberWithCommas(researchIncrease) or "N/A"))

        local warExhaustionInstance = powerFolder and powerFolder:FindFirstChild("WarExhaustion")
        local warExhaustionPower = getValue(powerFolder, "WarExhaustion")
        WarExhaustionLabel:SetText("War Exhaustion = " .. (warExhaustionPower ~= nil and formatNumberWithCommas(warExhaustionPower) or "N/A"))
        local warExhaustionIncrease = getValue(warExhaustionInstance, "Increase")
        WarExhaustionIncreaseLabel:SetText("War Exhaustion (Increase) = " .. (warExhaustionIncrease ~= nil and formatNumberWithCommas(warExhaustionIncrease) or "N/A"))

        local manpowerValue = getValue(countryDataFolder, "Manpower")
        ManpowerLabel:SetText("Manpower = " .. (manpowerValue ~= nil and formatNumberWithCommas(manpowerValue) or "N/A"))


        -- Data Stats
        local stabilityInstance = dataFolder and dataFolder:FindFirstChild("Stability")
        local stabilityValue = getValue(dataFolder, "Stability")
        StabilityLabel:SetText("Stability = " .. (stabilityValue ~= nil and formatNumberWithCommas(stabilityValue) or "N/A"))

        local stabilityChange = getValue(stabilityInstance, "Change")
        StabilityChangeLabel:SetText("Stability (Change) = " .. (stabilityChange ~= nil and formatNumberWithCommas(stabilityChange) or "N/A"))

        local goalInstance = stabilityInstance and stabilityInstance:FindFirstChild("Goal")
        local stabilityGoal = getValue(stabilityInstance, "Goal")
        StabilityGoalLabel:SetText("Stability (Goal) = " .. (stabilityGoal ~= nil and formatNumberWithCommas(stabilityGoal) or "N/A"))

        local stabilityBase = getValue(goalInstance, "Base")
        StabilityBaseLabel:SetText("Stability (Base) = " .. (stabilityBase ~= nil and formatNumberWithCommas(stabilityBase) or "N/A"))

        local populationValue = getValue(countryDataFolder, "Population")
        PopulationLabel:SetText("Population = " .. (populationValue ~= nil and formatNumberWithCommas(populationValue) or "N/A"))

        local populationGrowthRate = getValue(populationFolder, "GrowthRate")
        PopulationGrowthRateLabel:SetText("Population (Growth Rate) = " .. (populationGrowthRate ~= nil and formatNumberWithCommas(populationGrowthRate) or "N/A"))

        local growthRateExistingPopulation = getValue(growthRateInstance, "Existing_Population", true)
        GrowthRateExistingPopulationLabel:SetText("Growth Rate: Existing Population = " .. (growthRateExistingPopulation ~= nil and formatNumberWithCommas(growthRateExistingPopulation) or "N/A"))

        local growthRateNaturalGrowth = getValue(growthRateInstance, "Natural_Growth", true)
        GrowthRateNaturalGrowthLabel:SetText("Growth Rate: Natural Growth = " .. (growthRateNaturalGrowth ~= nil and formatNumberWithCommas(growthRateNaturalGrowth) or "N/A"))

        local growthRateStability = getValue(growthRateInstance, "Stability", true)
        GrowthRateStabilityLabel:SetText("Growth Rate: Stability = " .. (growthRateStability ~= nil and formatNumberWithCommas(growthRateStability) or "N/A"))

        local rankingValue = getValue(countryDataFolder, "Ranking")
        RankingLabel:SetText("Ranking = " .. (rankingValue ~= nil and formatNumberWithCommas(rankingValue) or "N/A"))


        -- Laws (Direct Children)
        local ideologyValue = getValue(lawsFolder, "Ideology")
        IdeologyLabel:SetText("Ideology = " .. (ideologyValue ~= nil and tostring(ideologyValue) or "N/A"))

