local WrappedLabel = LeftGroupbox:AddLabel({
    Text = "Normal SoundTrack",
    DoesWrap = true
})

local autoLooped = false -- Default to not looped

-- Helper function to create a button with common notification logic
-- This function now dynamically sets the 'Looped' parameter based on the autoLooped global variable.
-- IMPORTANT: This assumes Library:Notify supports a 'Looped' boolean parameter.
local function createSoundButton(groupbox, text, title, description, time, soundId)
    groupbox:AddButton({
        Text = text,
        Func = function()
            Library:Notify({
                Title = title,
                Description = description,
                Time = time,
                SoundId = soundId,
                Looped = autoLooped -- Pass the global loop state to the notification
            })
        end,
    })
end

