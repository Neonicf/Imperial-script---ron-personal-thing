        -- Other Country Data
        local c3Value = getValue(countryDataFolder, "C3")
        C3Label:SetText("C3 = " .. (c3Value ~= nil and tostring(c3Value) or "N/A"))

        local capitalValue = getValue(countryDataFolder, "Capital")
        CapitalLabel:SetText("Capital = " .. (capitalValue ~= nil and tostring(capitalValue) or "N/A"))

        local leaderValue = getValue(countryDataFolder, "Leader")
        LeaderLabel:SetText("Leader = " .. (leaderValue ~= nil and tostring(leaderValue) or "N/A"))

        local skinValue = getValue(countryDataFolder, "Skin")
        SkinLabel:SetText("Skin = " .. (skinValue ~= nil and tostring(skinValue) or "N/A"))

    else
        -- Reset all labels to "N/A (Country not found)"
        TreasuryLabel:SetText("Treasury/Balance = " .. notFoundText)
        IncomeRevenueLabel:SetText("Income/Revenue = " .. notFoundText)
        TaxLabel:SetText("Tax = " .. notFoundText)
        TaxResourceLabel:SetText("TaxResource (resources) = " .. notFoundText)
        TradeExportLabel:SetText("TradeExport = " .. notFoundText)
        ReparationsRevenueLabel:SetText("Reparations = " .. notFoundText)
        PuppetsRevenueLabel:SetText("Puppets = " .. notFoundText)

        UpkeepLabel:SetText("Upkeep = " .. notFoundText)
        ReinforcementsLabel:SetText("Reinforcements = " .. notFoundText)
        TradeImportLabel:SetText("TradeImport = " .. notFoundText)
        ReparationsExpensesLabel:SetText("Reparations = " .. notFoundText)
        PuppetsExpensesLabel:SetText("Puppets = " .. notFoundText)
        BuildingExpensesLabel:SetText("Building Expenses = " .. notFoundText)

        GovernmentSpendingLabel:SetText("Government Spending (Economy) = " .. notFoundText)
        SecuritySpendingLabel:SetText("Security Spending (Economy) = " .. notFoundText)
        EducationSpendingLabel:SetText("Education Spending (Economy) = " .. notFoundText)
        AntiCorruptionSpendingLabel:SetText("Anti-Corruption Spending (Economy) = " .. notFoundText)
        HealthcareSpendingLabel:SetText("Healthcare Spending (Economy) = " .. notFoundText)
        ResearchSpendingLabel:SetText("Research Spending (Economy) = " .. notFoundText)
        ReconstructionSpendingLabel:SetText("Reconstruction Spending (Economy) = " .. notFoundText)

        CorruptionLabel:SetText("Corruption = " .. notFoundText)
        CorruptionIncreaseLabel:SetText("Corruption (Increase) = " .. notFoundText)
        MilitaryLabel:SetText("Military = " .. notFoundText)
        MilitaryIncreaseLabel:SetText("Military (Increase) = " .. notFoundText)
        PoliticalPowerLabel:SetText("Political Power = " .. notFoundText)
        PoliticalPowerIncreaseLabel:SetText("Political Power (Increase) = " .. notFoundText)
        ReadinessLabel:SetText("Readiness = " .. notFoundText)
        ReadinessIncreaseLabel:SetText("Readiness (Increase) = " .. notFoundText)
        ResearchPowerLabel:SetText("Research (Power) = " .. notFoundText)
        ResearchPowerIncreaseLabel:SetText("Research (Power Increase) = " .. notFoundText)
        WarExhaustionLabel:SetText("War Exhaustion = " .. notFoundText)
        WarExhaustionIncreaseLabel:SetText("War Exhaustion (Increase) = " .. notFoundText)
        ManpowerLabel:SetText("Manpower = " .. notFoundText)

        StabilityLabel:SetText("Stability = " .. notFoundText)
        StabilityChangeLabel:SetText("Stability (Change) = " .. notFoundText)
        StabilityGoalLabel:SetText("Stability (Goal) = " .. notFoundText)
        StabilityBaseLabel:SetText("Stability (Base) = " .. notFoundText)
        PopulationLabel:SetText("Population = " .. notFoundText)
        PopulationGrowthRateLabel:SetText("Population (Growth Rate) = " .. notFoundText)
        GrowthRateExistingPopulationLabel:SetText("Growth Rate: Existing Population = " .. notFoundText)
        GrowthRateNaturalGrowthLabel:SetText("Growth Rate: Natural Growth = " .. notFoundText)
        GrowthRateStabilityLabel:SetText("Growth Rate: Stability = " .. notFoundText)
        RankingLabel:SetText("Ranking = " .. notFoundText)

        IdeologyLabel:SetText("Ideology = " .. notFoundText)
        ConscriptionLabel:SetText("Conscription = " .. notFoundText)
        TaxationLawsLabel:SetText("Taxation (Laws) = " .. notFoundText)

        PoliticalConscriptionLabel:SetText("Political: Conscription = " .. notFoundText)
        PoliticalFactionLabel:SetText("Political: Faction = " .. notFoundText)
        PoliticalFirearmLabel:SetText("Political: Firearm = " .. notFoundText)
        PoliticalIndustryLabel:SetText("Political: Industry = " .. notFoundText)
        PoliticalPopulationLabel:SetText("Political: Population = " .. notFoundText)
        PoliticalPressLabel:SetText("Political: Press = " .. notFoundText)
        PoliticalReligionLabel:SetText("Political: Religion = " .. notFoundText)
        PoliticalResourceLabel:SetText("Political: Resource = " .. notFoundText)
        PoliticalUniqueMonarchyLabel:SetText("Political: Unique_Monarchy = " .. notFoundText)
        PoliticalWarBondLabel:SetText("Political: WarBond = " .. notFoundText)
        PoliticalWorkingLabel:SetText("Political: Working = " .. notFoundText)

        SpendingCorruptionLabel:SetText("Spending (Laws): Corruption = " .. notFoundText)
        SpendingEducationLabel:SetText("Spending (Laws): Education = " .. notFoundText)
        SpendingGovernmentLabel:SetText("Spending (Laws): Government = " .. notFoundText)
        SpendingHealthcareLabel:SetText("Spending (Laws): Healthcare = " .. notFoundText)
        SpendingMilitaryLabel:SetText("Spending (Laws): Military = " .. notFoundText)
        SpendingReconstructionLabel:SetText("Spending (Laws): Reconstruction = " .. notFoundText)
        SpendingResearchLabel:SetText("Spending (Laws): Research = " .. notFoundText)
        SpendingSecurityLabel:SetText("Spending (Laws): Security = " .. notFoundText)

        C3Label:SetText("C3 = " .. notFoundText)
        CapitalLabel:SetText("Capital = " .. notFoundText)
        LeaderLabel:SetText("Leader = " .. notFoundText)
        SkinLabel:SetText("Skin = " .. notFoundText)
    end
end

-- Function to start or stop the automatic refresh loop
local function startOrStopAutoRefresh()
    -- Always cancel the existing task first to prevent duplicates or orphaned tasks
    if currentRefreshTask then
        task.cancel(currentRefreshTask)
        currentRefreshTask = nil
    end

    -- If auto-refresh is enabled AND there's a valid country name stored, start a new loop
    if autoRefreshEnabled and currentCountryNameBeingRefreshed ~= "" then
        -- Capture the country name for this specific loop instance
        local countryNameToLoop = currentCountryNameBeingRefreshed
        currentRefreshTask = task.spawn(function()
            -- The loop continues as long as the global tracker matches the country name
            -- that this specific loop instance was started for.
            while currentCountryNameBeingRefreshed == countryNameToLoop do
                refreshCountryStats(countryNameToLoop)
                task.wait(7) -- Wait for 7 seconds before the next refresh
            end
        end)
    end
end

-- Input field Callback
local Input = LeftGroupbox:AddInput("CountryNameInput", {
    Text = "Search up country",
    Default = "",
    Numeric = false,
    Finished = true,
    Placeholder = "Enter Countryname >:33",
    Tooltip = "This is capital sensitive and you have to enter it exactly (especially in custom servers)",
    ClearTextOnFocus = false,
    Callback = function(Value)
        local countryName = Value:gsub("^%s+", ""):gsub("%s+$", "") -- Trim whitespace

        -- Update the global tracker for the country name being refreshed
        currentCountryNameBeingRefreshed = countryName

        -- Immediately refresh the stats based on the new input
        refreshCountryStats(countryName)

        -- Evaluate and start/stop the auto-refresh loop based on current conditions
        startOrStopAutoRefresh()
    end
})

-- Auto-Refresh Toggle
local AutoRefreshCheckbox = LeftGroupbox:AddToggle("Auto-Refresh Stats", {
    Text = "Auto-Refresh Stats",
    Tooltip = "This will refresh the stats every 7 seconds, toggleable",
    Default = true, -- Default to auto-refresh enabled
    Callback = function(Value)
        autoRefreshEnabled = Value -- Update the global flag
        startOrStopAutoRefresh() -- Re-evaluate and start/stop the loop
    end
})

local MainTab = Window:AddTab("AutoHost", "screen-share") -- Second parameter is the icon name (optional)

local LeftGroupbox = MainTab:AddLeftGroupbox("Auto Events/Message", "wrench")

LeftGroupbox:AddButton({
    Text = "AutoHost Anschlus event",
    Func = function()
        -- The button acts as a start/stop for the event listener
        if not dateConnection then
            -- Starts listening for the date
            local vipConsole = workspace:WaitForChild("GameManager"):WaitForChild("VipConsole")
            print("Event activated. Waiting for the date.")
            
            dateConnection = workspace.CountryManager.Date.Actual.Changed:Connect(function()
                local date = workspace.CountryManager.Date.Actual.Value
                if date.X == 3 and date.Y == 12 and date.Z == 1938 then
                    print("Date reached! Firing the event.")
                    vipConsole:FireServer("CustomMessage", {
                        Message = "Anschlusus turssss die austria",
                        Mode = "Notification",
                        Title = "Anschlusus Test for a Script",
                        Button = "Bboiiiiii"
                    })
                    -- After firing, the listener disconnects to prevent it from firing again
                    dateConnection:Disconnect()
                    dateConnection = nil
                end
            end)
        else
            -- Stops listening for the date
            dateConnection:Disconnect()
            dateConnection = nil
            print("Event deactivated.")
        end
    end
})

local MainTab = Window:AddTab("AudioPlayer", "file-audio") -- Second parameter is the icon name (optional)

